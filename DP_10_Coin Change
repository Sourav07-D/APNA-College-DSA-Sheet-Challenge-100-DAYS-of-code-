class Solution {
    public int coinChange(int[] coins, int amount) {
        int[][] dp=new int[coins.length+1][amount+1];

        for(int i=1;i<dp.length;i++)
        {
            for(int j=1;j<dp[0].length;j++)
            {
                int val=coins[i-1];
                if(val<=j)
                {
                    dp[i][j]=Mat
                }
            }
        }
    }
}
